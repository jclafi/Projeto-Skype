<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC

   "-//Hibernate/Hibernate Mapping DTD 3.0//EN"

   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="modal">

    <class name="Erros_Skype" table="erros_skype">
        <id name="id_geral" column="id_geral" type="long"/>
        
        <property name="id" column="id" type="long"/>
        <property name="content" column="content" type="string"/>
        <property name="ip_adress" column="ip_adress" type="string"/>                
        <property name="host_name" column="host_name" type="string"/>
        <property name="error_date" column="error_date" type="timestamp"/>
        <property name="account_name" column="account_name" type="string"/>
    </class>

</hibernate-mapping>