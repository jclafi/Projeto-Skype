<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC

   "-//Hibernate/Hibernate Mapping DTD 3.0//EN"

   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="modal">

    <class name="Mensagens_Skype" table="mensagens_skype">
        <id name="id_geral" column="id_geral" type="long"/>
        
        <property name="id" column="id" type="integer" not-null="true"/>
        <property name="id_sender" column="id_sender" type="string"/>
        <property name="sender_display_name" column="sender_display_name" type="string"/>
        <property name="content" column="content" type="string"/>                
        <property name="chat" column="chat" type="string"/>
        <property name="message_type" column="message_type" type="string"/> 
        <property name="message_date" column="message_date" type="date"/>
        <property name="ip_adress" column="ip_adress" type="string"/> 
        <property name="host_name" column="host_name" type="string"/> 
        <property name="account_logged" column="account_logged" type="string"/> 
    </class>

</hibernate-mapping>